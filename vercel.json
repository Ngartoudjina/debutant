{
  "version": 2,
  "builds": [
    {
      "src": "Harmine/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "Harmine/dist"
      }
    },
    {
      "src": "server/api/**/*.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Harmine/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}